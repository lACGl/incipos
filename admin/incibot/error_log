        SELECT 
            us.id, 
            us.ad AS urun_adi, 
            SUM(sfd.miktar) AS toplam_satis,
            SUM(sfd.toplam_tutar) AS toplam_tutar
        FROM 
            satis_fatura_detay sfd
        JOIN 
            satis_faturalari sf ON sfd.fatura_id = sf.id
        JOIN 
            urun_stok us ON sfd.urun_id = us.id
        WHERE 
            sf.islem_turu = 'satis' AND YEAR(sf.fatura_tarihi) = 2025 AND MONTH(sf.fatura_tarihi) = 04
        GROUP BY 
            sfd.urun_id
        ORDER BY 
            toplam_satis DESC
        LIMIT 5
