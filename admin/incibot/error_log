            SELECT 
                m.id,
                CONCAT(m.ad, ' ', m.soyad) AS musteri_adi,
                m.telefon,
                COUNT(sf.id) AS toplam_islem,
                SUM(sf.toplam_tutar) AS toplam_harcama
            FROM 
                musteriler m
            LEFT JOIN 
                satis_faturalari sf ON m.id = sf.musteri_id
            WHERE 
                m.durum = 'aktif' AND
                sf.islem_turu = 'satis' AND YEAR(sf.fatura_tarihi) = 2025 AND MONTH(sf.fatura_tarihi) = 04
            GROUP BY 
                m.id
            ORDER BY 
                toplam_harcama DESC
            LIMIT 10
